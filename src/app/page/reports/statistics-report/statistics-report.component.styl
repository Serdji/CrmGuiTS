.distribution-report
  display: flex;

  &__stepper
    flex: 1;

  &__pdf-viewer
    flex: 2;
    text-align: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;

  &__dir,
  &__template
    color: #464646;
    cursor: pointer;

  &__dir
    font-size 16px;


  &__paginator
    margin-bottom: 10px;

  &__button
    margin-right: 15px;



.example-tree
  mat-icon
    color: #464646;

.example-tree-invisible {
  display: none;
}

.example-tree ul,
.example-tree li {
  margin-top: 0;
  margin-bottom: 0;
  list-style-type: none;
}