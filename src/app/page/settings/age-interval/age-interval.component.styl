.age-interval

  &__form
    display: flex;
    flex-direction: row;

  &__box
    flex: 1;
    display: flex;
    flex-direction: row;

  &__input
    width: 100%;
    margin-right: 20px;

    &:nth-child( 3 )
      margin-right: 0;

  &__row

    &-items
      height: 40px;
      align-items: center;
      display: flex;
      flex-direction: row;
      border-bottom: 1px solid rgba(0, 0, 0, 0.12);

      span
        height: 20px;
        flex: 1;
